alias ot='cp $FOMEL/ot.c $MAD_COMPILE; cd $MAD_COMPILE; scons; cp $MAD_COMPILE/ot.exe $FOMEL; cd $FOMEL'
alias el='cp $FOMEL/myelastic.c $MAD_COMPILE; cd $MAD_COMPILE; scons; cp $MAD_COMPILE/myelastic.exe $FOMEL; cd $FOMEL'
alias go='ot; el; python $FOMEL/SConstruct'
alias g='rm $FOMEL/*.exe; rm $MAD_COMPILE/*.exe; go'
